<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.umairthecw"
       name="ZemTV.com"
       version="3.4.0"
       provider-name="shani">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.urlresolver" version="1.0.0"/>
        <import addon="plugin.video.youtube" version="2.9.2"/>
        <import addon="script.module.simplejson" />
    </requires>
    <extension point="xbmc.python.pluginsource"
               library="addon.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>THE CW Client</summary>
        <description>Watch CW  on your Kodi. This plugin fetches the video sources CW Network and play on Kodi compatible plateforms</description>
        <platform>all</platform>
    </extension>
</addon>